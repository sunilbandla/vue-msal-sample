{
  "name": "vue-msal-sample",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Sunil B",
  "license": "MIT",
  "private": true,
  "homepage": "https://sunilbandla.github.io/vue-msal-sample",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "preparedist": "cross-env NODE_ENV=production mkdir -p dist/dist && find dist -maxdepth 1 -type f -exec mv {} dist/dist ; && cp index.html dist/",
    "predeploy": "npm run build && npm run preparedist",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "msal": "^1.0.2",
    "vue": "^2.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "gh-pages": "^1.1.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
